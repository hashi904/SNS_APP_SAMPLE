= render 'share/header'
.all-container
  .right
    = render 'share/right-sidebar'
    = render 'share/right-trend'
  .main
    .main-content
      .main-content__title
        ホーム
      .main-content-func-tweet
        = link_to 'ここでツイートする',"#" ,class: "main-content-func-tweet__link" ,method: :get

      .main-content-tweet
        .main-content-tweet-user
          = image_tag('member_photo_noimage.png', class: 'main-content-tweet-user__img')
        .main-content-tweet-user-box
          .main-content-tweet-user-box-info
            .main-content-tweet-user-box-info-name
              test
            .main-content-tweet-user-box-info-username
              @test
            .main-content-tweet-user-box-info-time
              1h
          .main-content-tweet-user-box-text
            test messages
          .main-content-tweet-user-box-func
            .main-content-tweet-user-box-func-message
              = link_to "#", class: "main-content-tweet-user-box-func-message__link" do
                = fa_icon 'comment',class: "tweet__fa-icon"
            .main-content-tweet-user-box-retweet
              = link_to "#", class: "main-content-tweet-user-box-retweet__link" do
                = fa_icon 'retweet',class: "tweet__fa-icon"
            .main-content-tweet-user-box-func-favorite
              = link_to "#", class: "main-content-tweet-user-box-func-favorite__link" do
                = fa_icon 'heart',class: "tweet__fa-icon"
            .main-content-tweet-user-box-func-copy
              = link_to "#", class: "main-content-tweet-user-box-func-copy__link" do
                = fa_icon 'upload',class: "tweet__fa-icon"
            .main-content-tweet-user-box-func-impression
              = link_to "#", class: "main-content-tweet-user-box-func-impression__link" do
                = fa_icon 'trash',class: "tweet__fa-icon"

